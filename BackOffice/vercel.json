{
    "buildCommand": "cd BackOffice && yarn install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/Backend/(.*)",
        "destination": "/Backend/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/client/$1"
      }
    ]
  }